{
  "name": "ui-validation-service",
  "version": "1.0.0",
  "description": "Data store that performs validation.",
  "main": "ui-validation-store.js",
  "scripts": {
    "test": "npm run jscs && npm run jshint && npm run build-tests && npm run testem",
    "jscs": "node_modules/.bin/jscs src",
    "jshint": "node_modules/.bin/jshint src",
    "build-tests": "node_modules/.bin/browserify --extension=.jsx test/shim.js test/validation-service-spec.js --transform babelify -d -o test/tests.js",
    "testem": "node_modules/.bin/testem ci"
  },
  "author": "SCDHHS",
  "license": "ISC",
  "peerDependencies": {
    "lodash": "3"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "es5-shim": "^4.1.10",
    "es6-promise": "^3.0.2",
    "jasmine": "^2.3.2",
    "jasmine-ajax": "^3.1.1",
    "jasmine-core": "^2.3.4",
    "jquery": "^2.1.4",
    "jscs": "2",
    "jshint": "2",
    "lodash": "3"
  }
}
